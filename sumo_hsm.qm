    QTimeEvt_rearm(&amp;me-&gt;timeEvtStuck, BSP_TICKS_PER_MILISSEC * parameters.is_stucked_timeout_ms);
} else {
    QTimeEvt_disarm(&amp;me-&gt;timeEvtStuck);
